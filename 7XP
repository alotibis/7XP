local Main6 = MakeTab({Name = "Info"})
local section = AddSection(Main6, {"Info"})
SetSection(section, "Info Player")

local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- الحصول على الصورة الرمزية للاعب
local userId = player.UserId
local thumbnailType = Enum.ThumbnailType.HeadShot
local thumbnailSize = Enum.ThumbnailSize.Size100x100
local content, isReady = Players:GetUserThumbnailAsync(userId, thumbnailType, thumbnailSize)

-- إضافة صورة اللاعب
local Image = AddImageLabel(Main6, {
  Name = "" .. player.Name,
  Image = content
})

local section = AddSection(Main6, {"superstaff"})
SetSection(section, "superstaff : 7c1l")



local section = AddSection(Main6, {"UserId"})
SetSection(section, "Player ID : ".. player.UserId)


local section = AddSection(Main6, {"Play Time"})
SetSection(section, "Play Time")

-- حساب مدة اللعب
local startTime = tick()

-- دالة لتحديث مدة اللعب
local function updatePlayTime()
    while true do
        local currentTime = tick()
        local playTime = currentTime - startTime
        local minutes = math.floor(playTime / 60)
        local seconds = math.floor(playTime % 60)
        SetSection(section, "مدة اللعب: " .. minutes .. " دقيقة و " .. seconds .. " ثانية")
        wait(1)
    end
end

-- بدء تحديث مدة اللعب
spawn(updatePlayTime)
